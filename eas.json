{
  "cli": {
    "version": ">= 0.52.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "API_URL": "https://api.swoome.fr/api"
      }
    },
    "preview": {
      "channel": "preview",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "API_URL": "https://api.swoome.fr/api"
      }
    },
    "production": {
      "channel": "production",
      "env": {
        "API_URL": "https://api.swoome.fr/api"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
