{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"baseUrl": "./src",
		"paths": {
			"@assets/*": ["./assets/*"],
			"@constants/*": ["./constants/*"],
			"@hooks/*": ["./hooks/*"],
			"@navigation/*": ["./navigation/*"],
			"@redux/*": ["./redux/*"],
			"@screens/*": ["./screens/*"],
			"@services/*": ["./services/*"],
			"@styles/*": ["./styles/*"],
			"@types/*": ["./types/*"],
			"@ui/*": ["./ui/*"],
			"@env": [".env"]
		}
	}
}
