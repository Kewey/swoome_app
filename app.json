{
  "expo": {
    "name": "Swoome",
    "slug": "swoome",
    "owner": "kewey65",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#090954"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/6d8ad49e-32a2-4e93-8ca3-c041458ea06c"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.kewey.swoome",
      "infoPlist": {
        "NSCameraUsageDescription": "$(PRODUCT_NAME) utilise ta caméra pour personnaliser ton avatar.",
        "NSPhotoLibraryUsageDescription": "$(PRODUCT_NAME) utilise tes fichiers pour personnaliser ton avatar.",
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#090954"
      },
      "versionCode": 20220816,
      "package": "com.kewey.swoome",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "$(PRODUCT_NAME) utilise tes fichiers pour personnaliser ton avatar.",
          "cameraPermission": "$(PRODUCT_NAME) utilise ta caméra pour personnaliser ton avatar."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./src/assets/images/favicon.png",
          "color": "#ffffff",
          "sounds": []
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
