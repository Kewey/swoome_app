{
  "expo": {
    "name": "Swoome",
    "slug": "swoome",
    "owner": "kewey65",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#090954"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/6d8ad49e-32a2-4e93-8ca3-c041458ea06c"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "buildNumber": "9",
      "bundleIdentifier": "com.kewey.swoome",
      "infoPlist": {
        "NSCameraUsageDescription": "We use camera to add personalization in user account like avatar."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#090954"
      },
      "versionCode": 9,
      "package": "com.kewey.swoome",
      "googleServicesFile": "./google-services.json",
      "permissions": ["android.permission.RECORD_AUDIO"]
    },
    "web": {
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Permettre à $(PRODUCT_NAME) d'accéder à vos photos.",
          "cameraPermission": "Permettre à $(PRODUCT_NAME) d'accéder à votre camera."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./src/assets/images/favicon.png",
          "color": "#ffffff",
          "sounds": []
        }
      ],
      "sentry-expo"
    ],
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
